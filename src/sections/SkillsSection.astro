---
import type { SkillsSection } from '@/types/content'

import SectionCard from '@/components/SectionCard.astro'
import SkillSet from '@/components/SkillSet.astro'

export interface Props extends SkillsSection {}

const { config, skillSets } = Astro.props
---

<SectionCard {...config}>
  <div class="flex flex-col gap-10">
    {skillSets.map((skillSet) => <SkillSet {...skillSet} />)}
  </div>
</SectionCard>
